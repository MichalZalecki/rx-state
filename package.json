{
  "name": "@rx-state/core",
  "version": "0.0.1",
  "description": "Connect rxjs to React component in Redux style... but without dispatch and constants.",
  "main": "lib/RxState.js",
  "directories": {
    "test": "test"
  },
  "ava": {
    "files": [
      "test/**/*.spec.js"
    ],
    "require": [
      "babel-register",
      "./test/setup/setupBrowser.js"
    ],
    "babel": "inherit",
    "timeout": "60s",
    "verbose": true
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-core": "6.8.0",
    "babel-eslint": "6.0.4",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.5.0",
    "enzyme": "^2.4.1",
    "eslint": "^2.9.0",
    "eslint-config-airbnb-base": "^3.0.0",
    "eslint-plugin-import": "^1.7.0",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.3.2",
    "react-dom": "^15.3.2",
    "rxjs": "^5.0.0-beta.12",
    "webpack": "1.13.0",
    "yargs": "4.7.0"
  },
  "scripts": {
    "build": "webpack --mode=build",
    "dev": "webpack --progress --colors --watch --mode=dev",
    "test": "ava",
    "test:watch": "ava --watch",
    "lint": "eslint src/**/*.js test/**/*.js",
    "clean": "rm -rf lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichalZalecki/rx-state.git"
  },
  "author": "Michał Załęcki <michal@michalzalecki.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MichalZalecki/rx-state/issues"
  },
  "homepage": "https://github.com/MichalZalecki/rx-state#readme"
}
